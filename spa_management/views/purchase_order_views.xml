<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="purchase_order_form_inherit_inspa" model="ir.ui.view">
        <field name="name">purchase.order.form.inherit.inspa</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form" />
        <field name="arch" type="xml">
            <field name="date_order" position="before">
                <field name="branch_id" required="1" />
            </field>

            <field name="partner_id" position="attributes">
                <attribute name="domain">[('supplier_rank', '&gt;', 0)]</attribute>
                <attribute name="context">{'res_partner_search_mode': 'supplier', 'show_vat': True,
                    'default_supplier_rank':1}</attribute>
            </field>
        </field>
    </record>

</odoo>
